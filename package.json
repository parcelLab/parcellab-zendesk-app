{
  "name": "parcellab-zendesk-app",
  "version": "1.0.0-beta.0",
  "description": "A Zendesk ticket sidebar app to check order status via parcelLab API",
  "author": "parcelLab",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "dev": "concurrently --kill-others \"npm run watch\" \"wait-on dist/manifest.json && npm run zat:server\"",
    "watch": "webpack --watch --mode=development",
    "zat:server": "zat server -p dist",
    "build:dev": "webpack --mode=development",
    "build": "webpack -p --mode=production",
    "lint": "standard"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@testing-library/dom": "^6.10.1",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^5.2.1",
    "precss": "^3.1.2",
    "standard": "^11.0.1",
    "wait-on": "^3.3.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@zendeskgarden/css-bedrock": "^7.0.31",
    "@zendeskgarden/react-buttons": "^7.0.2",
    "@zendeskgarden/react-forms": "^7.0.2",
    "@zendeskgarden/react-grid": "^7.0.2",
    "@zendeskgarden/react-notifications": "^7.0.2",
    "@zendeskgarden/react-tables": "^7.0.2",
    "@zendeskgarden/react-theming": "^7.0.0",
    "@zendeskgarden/react-typography": "^7.0.0",
    "core-js": "^3.3.6",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.1"
  }
}
